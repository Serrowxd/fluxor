{
  "name": "@fluxor/cache-module",
  "version": "1.0.0",
  "description": "Distributed caching module for Fluxor with Redis support",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "cache",
    "redis",
    "distributed-cache",
    "fluxor"
  ],
  "dependencies": {
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "redis-mock": "^0.56.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}